
// const function arg example

fn add args x as const int, y as const int return int {
    x y +
}

fn main {
    34 35 add .
    "\n" .

    vec[int]
    use x {
        x
        dup
        dup 5 vec:push
        .
        "\n" .
        .
        "\n" .
    }

    vec[int]
    use x {
        x
        copy
        dup 5 vec:push
        .
        "\n" .
        .
        "\n" .
    }

}
