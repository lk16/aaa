struct Bar {
    value as int,
}

fn Bar:increment args bar as Bar {
    bar "value" { bar "value" ? 1 + } !
}

fn main {
    Bar
    use bar {
        bar "value" { 68 } !
        "Bar:increment" fn
        use increment {
            bar increment call
        }
        bar "value" ? .
    }
    "\n" .
}
