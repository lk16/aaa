enum OptionalInt {
    none,
    some as int,
}

fn main {
    "OptionalInt:none" fn
    use optional_int_none {
        optional_int_none call
        match {
            case OptionalInt:none { nop }
            default { unreachable }
        }
    }

    "OptionalInt:some" fn
    use optional_int_some {
        69 optional_int_some call
        match {
            case OptionalInt:some { . }
            default { unreachable }
        }
    }
    "\n" .
}
